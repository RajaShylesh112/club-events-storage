# MongoDB Configuration
# Replace with your MongoDB connection string
DB_URI=mongodb+srv://username:password@cluster.mongodb.net/database-name?retryWrites=true&w=majority
DB_NAME=your-database-name

# JWT Configuration
# Generate a secure secret key for JWT tokens
JWT_SECRET=your-super-secret-jwt-key-change-this-in-production

# Google OAuth2 Configuration
# Get these from Google Cloud Console (https://console.cloud.google.com/)
GOOGLE_CLIENT_ID=your-google-client-id
GOOGLE_CLIENT_SECRET=your-google-client-secret
GOOGLE_REDIRECT_URI=http://localhost:8000/auth/callback

# FastAPI Configuration
# Generate secure secret keys
SECRET_KEY=your-super-secret-fastapi-key-change-this-in-production
SESSION_SECRET_KEY=your-session-secret-key-change-this-in-production
ENVIRONMENT=development

# Optional: Backblaze B2 Configuration (for file storage)
B2_KEY=your_backblaze_key
B2_SECRET=your_backblaze_secret
